Algoritmo "Contageminteligente"
Var
   i, f, cont: inteiro
Inicio
      escreval("Inicio: ")
      leia(i)
      escreval("Fim: ")
      leia(f)
      escreval("-- CONTANDO --")
      cont <- i
      enquanto (cont <= f) faca
               escreva("",cont,"...")
               cont <- cont + 1
      fimenquanto
Fimalgoritmo